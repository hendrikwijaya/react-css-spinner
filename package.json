{
  "name": "react-css-spinner",
  "version": "1.0.0",
  "description": "CSS only spinner for react",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hendrikwijaya/react-css-spinner.git"
  },
  "keywords": [
    "react",
    "css",
    "spinner"
  ],
  "author": "Hendrik Wijaya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hendrikwijaya/react-css-spinner/issues"
  },
  "homepage": "https://github.com/hendrikwijaya/react-css-spinner#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.5.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {
    "react": "^16.8.4"
  },
  "peerDependencies": {
    "react": ">=16.2"
  },
  "engines": {
    "node": ">=8"
  }
}
